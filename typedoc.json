{
  "entryPointStrategy": "expand",
  "entryPoints": ["src/scripts/**/*.ts"],
  "out": "docs",
  "name": "NEW H.E.R.O. - Documentación Técnica",
  "includeVersion": true,
  "readme": "GAME_OVERVIEW.md",
  "exclude": [
    "**/*.test.ts",
    "**/node_modules/**",
    "**/dist/**"
  ],
  "excludePrivate": false,
  "excludeProtected": false,
  "excludeInternal": false,
  "theme": "default",
  "categorizeByGroup": true,
  "categoryOrder": [
    "Core",
    "Components",
    "Utils",
    "Solid"
  ],
  "navigation": {
    "includeCategories": true,
    "includeGroups": true
  },
  "validation": {
    "invalidLink": true,
    "notDocumented": false
  },
  "githubPages": false,
  "gitRevision": "main"
}

