{
  "appId": "com.newhero.game",
  "appName": "HERO Game",
  "webDir": "dist",
  "bundledWebRuntime": false,
  "android": {
    "allowMixedContent": true,
    "captureInput": true,
    "webContentsDebuggingEnabled": true,
    "useWebView": true,
    "allowMixedContentOverride": true,
    "webViewDebuggingEnabled": true,
    "iFrameSupport": true
  },
  "plugins": {
    "StatusBar": {
      "style": "dark",
      "backgroundColor": "#000000",
      "overlaysWebView": true
    },
    "App": {
      "launchUrl": "com.newhero.game://",
      "deepLinkScheme": "com.newhero.game"
    }
  }
}
