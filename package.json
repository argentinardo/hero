{
  "name": "mi-proyecto",
  "version": "1.0.0",
  "description": "Un proyecto TypeScript b√°sico",
  "main": "dist/bundle.js",
  "scripts": {
    "build": "npm run docs:generate && webpack --mode=production && npm run docs:copy-to-dist",
    "build:web": "npm run docs:generate && webpack --mode=production && npm run docs:copy-to-dist",
    "docs:copy-to-dist": "node scripts/copy-docs.js",
    "build:android": "npm run build:web && npx cap sync android && npx cap open android",
    "build:all": "npm run build:web && npx cap sync",
    "dev": "webpack serve",
    "cap:add:android": "npx cap add android",
    "cap:sync": "npx cap sync",
    "cap:open:android": "npx cap open android",
    "docs:generate": "typedoc --out docs src/scripts",
    "docs:serve": "npm run docs:generate && npx http-server docs -p 8081 -o"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@capacitor/android": "^7.4.3",
    "@capacitor/cli": "^7.4.3",
    "@capacitor/core": "^7.4.3",
    "@types/express": "^5.0.3",
    "css-loader": "^7.1.2",
    "express": "^5.1.0",
    "html-webpack-plugin": "^5.6.4",
    "sass": "^1.93.0",
    "sass-loader": "^16.0.5",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.4",
    "typedoc": "^0.28.14",
    "typedoc-plugin-markdown": "^4.9.0",
    "typescript": "^5.0.0",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "dependencies": {
    "@neondatabase/serverless": "^0.9.3",
    "@netlify/blobs": "^6.5.0",
    "@netlify/neon": "0.1.0",
    "nipplejs": "^0.10.2"
  }
}
